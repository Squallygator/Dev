@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Tests</title>
    <link rel="shortcut icon" type="image/png" href="~/Scripts/jasmine/jasmine_favicon.png">
    <link rel="stylesheet" type="text/css" href="~/Scripts/jasmine/jasmine.css">
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="~/Scripts/jasmine/jasmine.js"></script>
    <script type="text/javascript" src="~/Scripts/jasmine/jasmine-html.js"></script>
    <script type="text/javascript" src="~/Scripts/jasmine/boot.js"></script>

    <!-- include source files here... -->
    <script src="~/Scripts/jquery-2.1.0.min.js"></script>
    <script src="~/Scripts/Common/Events.js"></script>
    <script src="~/Scripts/Common/jQuery.Das.Data.js"></script>

    <!-- include spec files here... -->
    <script type="text/javascript" src="~/Scripts/Specs/jQuery_Das_Data_Specs.js"></script>

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("SimpleSearch", "Index", "Default", null, new { @class = "navbar-brand" })
                @Html.ActionLink("Tests Jasmine", "Tests", "Default", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>
</body>
</html>
