<Window x:Class="ReorganizeFilesInDirectory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox HorizontalAlignment="Left" Margin="10,23,0,0" VerticalAlignment="Top" Width="120" ItemsSource="{Binding Path=reorganizeAction, Mode=TwoWay, ElementName=mainWindow}" DisplayMemberPath="Value" SelectedValuePath="Key">
        </ComboBox>

    </Grid>
</Window>
